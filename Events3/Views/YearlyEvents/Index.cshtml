@model IEnumerable<Events3.Models.YearlyEvents>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            
            
            <th>
                @Html.DisplayNameFor(model => model.SpecialEvents)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SpecialShabassos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Months)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Parshios)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartTime)
            </th>
            
            <th>
                @Html.DisplayNameFor(model => model.EndTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LifeEvents)
            </th>

        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.SpecialEvents)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SpecialShabassos)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Months)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Parshios)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LifeEvents)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EventDescription)
        </td>

        <td>
            <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> <br>
            <a asp-action="Details" asp-route-id="@item.Id">Details</a> <br>
            <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
        </td>
    </tr>
   
}
    </tbody>
</table>
